{
	"version": 2,
	"builds": [
		{
			"src": "dist/main.js",
			"use": "@vercel/node"
		}
	],
	"routes": [
		{
			"src": "/(.*)",
			"dest": "/dist/main.js",
			"headers": {
				"Content-Security-Policy": "default-src 'self'; script-src 'self' 'unsafe-inline' https://vercel.live; connect-src 'self' https://vercel.live;"
			}
		}
	]
}
